<!DOCTYPE html>
<html>
  <head>
    <title>CfaCoffeeChat</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'layouts/nav' %>
    <div class="container">
      <% if notice %>
        <p><%= notice %></p>
      <% end %>

      <%= yield %>
    </div>
  </body>
</html>
